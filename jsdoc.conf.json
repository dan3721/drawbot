{
  "opts": {
    "destination": "api",
    "template": "node_modules/foodoc/template"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "recurseDepth": 10,
  "source": {
    "include": [
      "src"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "systemName": "Mr. Drawbot",
    "systemSummary": "_What's the matter with this thing? What's all this churning and bubbling, you call that a radar screen? - Dark Helmet_",
    "search": false,
    "sort": "longname, version, since, linenum"
  }
}